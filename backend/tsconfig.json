{
    "compilerOptions": {
      "typeRoots": ["./node_modules/@types"],
      "types": ["node"],
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
    },
    "include": ["src/**/*.ts", "src/routes/auth_routes.ts"],
    "exclude": ["node_modules"]
  }
  